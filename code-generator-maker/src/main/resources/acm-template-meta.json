{
  "id": 5,
  "name": "acm-template-pro-generator-5",
  "description": "ACM 示例模板生成器",
  "basePackage": "com.code",
  "version": "1.0",
  "author": "Liang",
  "createTime": "",
  "fileConfig": {
    "sourceRootPath": "E:/projects/code-generator/code-generator-demo-projects/acm-template-pro",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/code/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/code/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "groupKey": "git",
        "groupName": "开源",
        "type": "group",
        "condition": "needGit",
        "files": [
          {
            "inputPath": ".gitignore",
            "outputPath": ".gitignore",
            "type": "file",
            "generateType": "static"
          },
          {
            "inputPath": "README.md",
            "outputPath": "README.md",
            "type": "file",
            "generateType": "static"
          }
        ]
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "loop",
        "type": "Boolean",
        "description": "是否生成循环,默认false",
        "defaultValue": false,
        "abbr": "l"
      },
      {
        "fieldName": "needGit",
        "type": "Boolean",
        "description": "是否生成 .gitignore README文件",
        "defaultValue": true
      },
      {
        "fieldName": "main",
        "type": "Boolean",
        "description": "是否开启配置模板参数",
        "defaultValue": true
      },
      {
        "groupKey": "mainTemplate",
        "groupName": "核心模板参数",
        "type": "MainTemplate",
        "description": "用于生成核心模板文件",
        "condition": "main",
        "models": [
          {
            "fieldName": "author",
            "type": "String",
            "description": "作者注释",
            "defaultValue": "author",
            "abbr": "a"
          },
          {
            "fieldName": "outputText",
            "type": "String",
            "description": "输出信息",
            "defaultValue": "sum = ",
            "abbr": "o"
          }
        ]
      }
    ]
  }
}